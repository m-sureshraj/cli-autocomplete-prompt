{
  "name": "cli-autocomplete-prompt",
  "version": "0.0.1",
  "description": "Fully customizable autocomplete prompt for CLIs",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=8.6"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "start": "npm run default-autocomplete --prefix examples",
    "test": "",
    "lint-all": "eslint '**/*.{js,ts}'",
    "lint-fix": "eslint --fix"
  },
  "prettier": "@sureshraj/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint-fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-sureshraj/cli-autocomplete-prompt.git"
  },
  "keywords": [
    "cli",
    "select-prompt",
    "autocomplete"
  ],
  "author": "Sureshraj <m.s.suresh100@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m-sureshraj/cli-autocomplete-prompt/issues"
  },
  "homepage": "https://github.com/m-sureshraj/cli-autocomplete-prompt#readme",
  "dependencies": {
    "ansi-escapes": "^4.3.1",
    "kleur": "^4.1.3",
    "strip-ansi": "^6.0.0"
  },
  "devDependencies": {
    "@sureshraj/prettier-config": "^1.0.0",
    "@types/node": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
